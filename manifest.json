{
	"manifest_version": 2,
	"name": "Hide Subscription Videos",
	"description": "Hide certain videos from your Subscription feed based on keywords",
	"version": "1.0.0",
	"icons": {
		"64": "icons/youtube.png"
	},
	"permissions": [
		"storage"
	],
	"browser_specific_settings": {
		"gecko": {
			"id": "hideSubscriptionVideos@fake-hideSubscriptionVideos.com"
		}
	},
	"content_scripts": [
		{
			"run_at": "document_end",
			"matches": [
				"*://*.youtube.com/feed/subscriptions"
			],
			"js": [
				"app.js",
				"popup/popup.js"
			]
		}
	],
	"browser_action": {
		"default_icon": "icons/youtube.png",
		"default_title": "Hide Subscription Videos",
		"default_popup": "popup/view.html"
	}
}